{
    "version": 2,
    "name": "discord-gpt",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "env": {
      "API_KEY": "@api_key",
      "CHANNEL_ID": "@channel_id",
      "TOKEN": "@token"
    }
  }
  